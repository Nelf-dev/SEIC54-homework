<h1> New Ocean</h1>

<form action="<%= oceans_path %>" method="post">
    <label>
    Name:
        <input name="name" required>
    </label>

    <label>
    Image:
    <input name="image" type="url" >
    </label>

    <label>
    Size:
    <input name="size" >
    </label>

    <label>
    Depth:
    <input name="depth" >
    </label>

    <label>
    Info:
    <input name="info" id="textbox" >
    </label>

    <button>Create Ocean</button>
</form>