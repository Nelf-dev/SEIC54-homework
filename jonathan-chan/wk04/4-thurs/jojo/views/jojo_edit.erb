<form action="/jojo/<%= @jojo["id"]%>" method="post">
    <label>
        First Name:
        <input name="first_name" required value="<%=
        @jojo["first_name"]%>">
    </label>

    <label>
        Last Name:
        <input name="last_name" value="<%=
        @jojo["last_name"]%>">
    </label>

    <label>
        Image:
        <input name="url" value="<%=
        @jojo["image"]%>">
    </label>

    <button>Update Jojo</button>

</form>