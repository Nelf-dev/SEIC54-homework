<%= form_for @artist do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, :required => true %>
    <%= f.label :nationality %>
    <%= f.text_field :nationality, :required => true %>
    <%= f.label :style %>
    <%= f.text_field :style, :required => true %>
    <%= f.label :image %>
    <%= f.url_field :image, :required => true %>
    <%= f.submit %>
<% end%>